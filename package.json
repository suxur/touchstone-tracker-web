{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-react": "^7.13.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-rc.0",
    "@storybook/addon-actions": "^6.5.0-alpha.3",
    "@storybook/addon-essentials": "^6.5.0-alpha.3",
    "@storybook/addon-links": "^6.5.0-alpha.3",
    "@storybook/addons": "^6.5.0-alpha.3",
    "@storybook/builder-webpack5": "^6.5.0-alpha.3",
    "@storybook/manager-webpack5": "^6.5.0-alpha.3",
    "@storybook/theming": "^6.5.0-alpha.3",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.37",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.11",
    "@types/ziggy-js": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.4",
    "react-refresh": "^0.11.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.44.0",
    "sass-loader": "^12.1.0",
    "tailwindcss": "^3.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "laravel-mix": "^6.0.6",
    "postcss-import": "^12.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@headlessui/react": "^1.4.2",
    "@inertiajs/inertia": "^0.10.1",
    "@inertiajs/inertia-react": "^0.7.1",
    "@inertiajs/progress": "^0.2.6",
    "@popperjs/core": "^2.11.2",
    "@react-hook/window-size": "^3.0.7",
    "@storybook/react": "^6.5.0-alpha.3",
    "axios": "^0.21",
    "clipboard-copy": "^4.0.1",
    "clsx": "^1.1.1",
    "immer": "^9.0.7",
    "laravel-echo": "^1.11.3",
    "lodash": "^4.17.21",
    "mathjs": "^10.0.0",
    "pusher-js": "^7.0.4",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.1",
    "react-moveable": "^0.30.3",
    "react-popper": "^2.2.5",
    "react-query": "^3.34.11",
    "webpack": "^5.65.0",
    "ziggy-js": "^1.4.2"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "react-hooks"
    ],
    "rules": {
      "arrow-parens": 0,
      "import/extensions": [
        2,
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "import/prefer-default-export": 0,
      "jsx-a11y/control-has-associated-label": "off",
      "jsx-a11y/label-has-associated-control": "off",
      "max-len": "off",
      "no-param-reassign": "off",
      "no-use-before-define": "off",
      "object-curly-newline": 0,
      "react/function-component-definition": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      ],
      "react/button-has-type": "off",
      "react/jsx-props-no-spreading": 0,
      "react/jsx-one-expression-per-line": "off",
      "react/prop-types": 0,
      "react/require-default-props": 0,
      "react-hooks/rules-of-hooks": 2,
      "react-hooks/exhaustive-deps": 1,
      "@typescript-eslint/no-explicit-any": "off"
    },
    "settings": {
      "import/resolver": {
        "eslint-import-resolver-custom-alias": {
          "alias": {
            "@": "./resources/js"
          },
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx",
            ".d.ts"
          ]
        }
      }
    }
  }
}
